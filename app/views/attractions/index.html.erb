<% @attractions.each do |attraction| %>
  <p>Name <%= attraction.name %></p>
  <p>Tickets <%= attraction.tickets %></p>
  <% if @user.admin %>
    <p>Details <%= link_to "Show #{attraction.name}", attraction_path(attraction) %> </p>
    <% else %>
    <p> Details <%= link_to "Go on #{attraction.name}", attraction_path(attraction) %> </p>
  <%end%>
<%end%>

<%= link_to "New Attraction", new_attraction_path %>